{
  "manifest_version": 3,
  "name": "Lichess analysis",
  "description": "Opens lichess analysis for games from chess.com",
  "version": "0.1.0",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://www.chess.com/game/live/*"],
  "background": {
    "service_worker": "open_lichess.js"
  },
  "action": {
    "default_icon": "/assets/img/lichess_icon.png"
  }
}
